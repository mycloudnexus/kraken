{
  "singleQuote": false,
  "semi": true,
  "endOfLine": "lf",
  "trailingComma": "es5",
  "importOrder": [
    "^[./]"
  ],
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "overrides": [
    {
      "files": "*.scss",
      "options": {
        "parser": "scss"
      }
    }
  ]
}
